)\r\n          .then(forecast =>\r\n            setForecastCities([...forecastCities, { ...forecast }])\r\n          )\r\n          .catch(error => console.error(error))\r\n          .finally(() => setIsLoading(false));\r\n      })\r\n      .catch(\r\n        // error => console.error(error)\r\n        () => alert(\"City not found\")\r\n      )\r\n      .finally(() => setIsLoading(false));\r\n  }, [searchCity]);\r\n\r\n  localStorage.setItem('weatherCities', JSON.stringify(weatherCities));\r\n  localStorage.setItem('forecastCities', JSON.stringify(forecastCities));\r\n\r\n  const onDeleteCard = cityId => {\r\n    setCityId(cityId);\r\n    setWeatherCities(weatherCities.filter(({ id }) => id !== cityId));\r\n    // console.log('WeatherCity weatherCities:', weatherCities);\r\n  };\r\n\r\n  return (\r\n    <WeatherBar dataOffset={offset}>\r\n      <BlockBtn>\r\n        <ListBtn\r\n          type=\"button\"\r\n          aria-label=\"list\"\r\n          title=\"List\"\r\n          onClick={() => {\r\n            setOnCloseBtn(!onCloseBtn);\r\n          }}\r\n        >\r\n          <CorrectListImg />\r\n        </ListBtn>\r\n        <CloseBtn\r\n          type=\"button\"\r\n          aria-label=\"close\"\r\n          title=\"Close\"\r\n          onClick={() => {\r\n            setOffset(-100);\r\n            setIsOpen(!isOpen);\r\n          }}\r\n        >\r\n          <WeatherCityClose />\r\n        </CloseBtn>\r\n      </BlockBtn>\r\n      <Title>Weather</Title>\r\n      <SearchBar setSearchCity={setSearchCity} weather={weather} />\r\n      <WeatherList onCloseBtn={onCloseBtn} onDeleteCard={onDeleteCard} />\r\n      {isLoading && <Loader />}\r\n    </WeatherBar>\r\n  );\r\n};\r\n","D:\\My Project\\weather-app\\src\\components\\WeatherCity\\WeatherCity.styled.js",[],[],"D:\\My Project\\weather-app\\src\\components\\App\\App.jsx",[],[],"D:\\My Project\\weather-app\\src\\components\\App\\App.styled.js",[],[],"D:\\My Project\\weather-app\\src\\components\\Searchbar\\Searchb
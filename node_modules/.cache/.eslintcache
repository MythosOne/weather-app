yId => {\r\n    setCityId(cityId);\r\n\r\n    setWeatherCities(weatherCities.filter(({ id }) => id !== cityId));\r\n    setForecastCities(forecastCities.filter(({ city }) => city.id !== cityId));\r\n  };\r\n\r\n  const content = (\r\n    <WeatherBar dataOffset={offset}>\r\n      <BlockBtn>\r\n        <ListBtn\r\n          type=\"button\"\r\n          aria-label=\"list\"\r\n          title=\"List\"\r\n          onClick={() => {\r\n            setOnCloseBtn(!onCloseBtn);\r\n          }}\r\n        >\r\n          <CorrectListImg />\r\n        </ListBtn>\r\n        <CloseBtn\r\n          type=\"button\"\r\n          aria-label=\"close\"\r\n          title=\"Close\"\r\n          onClick={() => {\r\n            setOffset(-100);\r\n            setIsOpen(!isOpen);\r\n          }}\r\n        >\r\n          <WeatherCityClose />\r\n        </CloseBtn>\r\n      </BlockBtn>\r\n      <Title>Weather</Title>\r\n      <SearchBar setSearchCity={setSearchCity} weather={weather} />\r\n      <WeatherList onCloseBtn={onCloseBtn} onDeleteCard={onDeleteCard} />\r\n      {isLoading && <Loader />}\r\n    </WeatherBar>\r\n  );\r\n\r\n  if (windowWidth < 767 && mobilePortal) {\r\n    return createPortal(content, mobilePortal);\r\n  }\r\n\r\n  return content;\r\n};\r\n","D:\\My Project\\weather-app\\src\\components\\WeatherCity\\WeatherCity.styled.js",[],[],"D:\\My Project\\weather-app\\src\\components\\App\\App.jsx",[],[],"D:\\My Project\\weather-app\\src\\components\\App\\App.styled.js",[],[],"D:\\My Project\\weather-app\\src\\components\\Searchbar\\Searchbar.jsx",[],[],"D:\\My Project\\weather-app\\src\\components\\Searchbar\\Searchbar.styled.js",[],[],"D:\\My Project\\weather-app\\src\\components\\Searchbar\\SearchBar.jsx",[],[],"D:\\My Project\\weather-app\\src\\components\\Searchbar\\SearchBar.styled.js",[],[],"D:\\My Project\\weather-app\\src\\components\\SearchBar\\SearchBar.jsx",[],[],"D:\\My Project\\weather-app\\src\\components\\SearchBar\\SearchBar.styled.js",[],[],"D:\\My Project\\